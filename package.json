{
  "name": "fullstack-launchpad-backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
        "start": "node dist/index.js",
        "dev": "nodemon src/index.ts",
        "build": "tsc",
        "postinstall": "npm run build",
        "ts.check": "tsc --project tsconfig.json",
        "add-build": "git add dist",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
        "lint": "eslint  --fix src/**/*.ts"
    },
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "pre-commit": "^1.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7"
  }
}
